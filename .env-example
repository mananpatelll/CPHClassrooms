DJANGO_SECRET_KEY=''
DJANGO_DEBUG=True
DJANGO_SECURE_SSL_REDIRECT=False


DATABASE_URL= add your postgres url if you have 
DJANGO_ALLOWED_HOSTS=127.0.0.1,localhost,[::1]
DJANGO_USE_REDIS=False

# Media storage: "local" for dev, "s3" for prod-style object storage
STORAGE_BACKEND=local

# If STORAGE_BACKEND=s3, set all of these:
S3_ACCESS_KEY=__set_me__
S3_SECRET_KEY=__set_me__
S3_BUCKET_NAME=ctsclassrooms-media
S3_REGION=us-east-1
S3_ENDPOINT_URL=https://s3.amazonaws.com


# Optional: serve media via CDN/vanity domain instead of the raw endpoint
S3_CUSTOM_DOMAIN=

# Addressing: "virtual" for most providers, "path" for some MinIO setups
S3_ADDRESSING_STYLE=virtual